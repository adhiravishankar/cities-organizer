{
  "name": "@adhiravishankar/colognes-website-mongo",
  "version": "0.1.0",
  "scripts": {
    "build": "redrun --series build:components build:parcel",
    "build:components": "redrun --parallel build:sass build:tsc",
    "build:sass": "sass src/scss/index.scss build/index.css",
    "build:tsc": "tsc",
    "build:parcel": "parcel build build/index.html",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "serve": "redrun --series build:components serve:parcel",
    "serve:parcel": "parcel build/index.html",
    "test": "redrun --serial test:e2e",
    "test:e2e": "cypress open",
    "watch": "redrun --series build:components watch:parcel",
    "watch:parcel": "parcel watch build/index.html"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@tanstack/react-table": "^8.7.6",
    "atomic-sleep": "^1.0.0",
    "ky": "^0.33.1",
    "material-colors": "^1.2.6",
    "material-react-table": "^1.5.4",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "queue-typescript": "^1.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.41.5",
    "react-markdown": "^8.0.4",
    "react-number-format": "^5.1.2",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2",
    "react-select": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "parcel": "^2.8.2",
    "process": "^0.11.10",
    "redrun": "^10.0.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  }
}
